{
    "require-dev": {
        "codeception/lib-asserts": "^2.0",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Turbine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TurbineTest\\": "tests/_support/",
            "TurbineTest\\Unit\\": "tests/unit/"
        }
    },
    "scripts": {
        "codecept": "php vendor/bin/codecept",
        "tests": "php vendor/bin/codecept run"
    }
}
